<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.somon.bdcache">

    <application
        android:allowBackup="true"
        android:icon="@drawable/logo"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.CONTROLE">

        <activity
            android:name="com.somon.bdcache.CAPTEUR"
            android:label="@string/capteur"
            android:theme="@style/Theme.CONTROLE.NoActionBar"></activity>

        <activity
            android:name="com.somon.bdcache.Capteurs"
            android:label="@string/capteurs"
            android:theme="@style/Theme.CONTROLE.NoActionBar"></activity>

        <activity
            android:name="com.somon.bdcache.CapteurValue"
            android:label="@string/CapteurValue"
            android:theme="@style/Theme.CONTROLE.NoActionBar"></activity>

        <activity
            android:name="com.somon.bdcache.Homepage"
            android:label="@string/Homepage"
            android:theme="@style/Theme.CONTROLE.NoActionBar"></activity>

        <activity
            android:name="com.somon.bdcache.Salle"
            android:label="@string/Salle"
            android:theme="@style/Theme.CONTROLE.NoActionBar"></activity>


        <activity android:name="com.somon.bdcache.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>